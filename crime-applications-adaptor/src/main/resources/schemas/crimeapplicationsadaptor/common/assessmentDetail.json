{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "assessmentDetail.json",
  "type": "object",
  "description": "Assessment details",
  "properties": {
    "applicantAmount": {
      "type": "number",
      "description": "An explanation about the purpose of this instance."
    },
    "applicantFrequency": {
      "type": "string",
      "description": "The Frequency of Applicant",
      "enum": ["WEEKLY", "2WEEKLY", "4WEEKLY", "MONTHLY", "ANNUALLY"]
    },
    "assessmentDetailCode": {
      "type": "string",
      "description": "Assessment Detail Code"
    }
  }
}
